{
	"name": "@ryoppippi/unocss-preset",
	"type": "module",
	"version": "2.0.1",
	"packageManager": "pnpm@10.12.4",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "https://github.com/ryoppippi/unocss-preset.git",
	"keywords": [
		"unocss",
		"ryoppippi"
	],
	"sideEffects": false,
	"exports": {
		".": {
			"type": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "unbuild --sourcemap",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:update": "vitest --update",
		"prepublishOnly": "nr build",
		"release": "nr lint && nr typecheck && nlx publint && nlx bumpp && pnpm publish"
	},
	"peerDependencies": {
		"unocss": ">=0.56.0 < 101"
	},
	"devDependencies": {
		"@antfu/ni": "^25.0.0",
		"@babel/types": "^7.27.7",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.25",
		"@types/node": "^24.0.4",
		"@unocss/eslint-plugin": "^66.3.2",
		"eslint": "~9.30.0",
		"eslint-plugin-format": "^1.0.1",
		"typescript": "5.8.3",
		"unbuild": "^3.5.0",
		"unocss": "^66.3.2",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"executionEnv": {
			"nodeVersion": "22.14.0"
		},
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
